{
  "name": "ts-react-app",
  "version": "0.19.0",
  "description": "TypeScript React app template",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": "^12.0.0"
  },
  "scripts": {
    "prestart": "npm run build",
    "start": "NODE_ENV=production cd ./build && npx serve",
    "dev": "webpack-dev-server --port 3000 --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "types:check": "tsc --noEmit",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx .",
    "lint:css": "stylelint \"src/**/*.css\"",
    "prettier": "prettier \"./src/**/*.{js,jsx,ts,tsx,css,html}\"",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --list-different",
    "test": "jest --forceExit",
    "test:update": "jest -u",
    "test:watch": "npm t -- --watch",
    "test:ci": "npm t -- --config jest.config.ci.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ink8bit/ts-react-app.git"
  },
  "author": "ink8bit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ink8bit/ts-react-app/issues"
  },
  "homepage": "https://github.com/ink8bit/ts-react-app#readme",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.19",
    "@types/reach__router": "^1.3.6",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-test-renderer": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.2.1",
    "react-test-renderer": "^16.13.1",
    "serve": "^11.3.2",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended": "^3.0.0",
    "terser-webpack-plugin": "^4.2.3",
    "typescript": "^4.0.5",
    "url-loader": "^4.1.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.3.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@reach/router": "^1.3.4",
    "classnames": "^2.2.6",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  }
}
