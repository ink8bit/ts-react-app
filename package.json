{
  "name": "ts-react-app",
  "version": "0.2.0",
  "description": "React app written with TypeScript",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --port 3000 --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "type:check": "tsc --noEmit",
    "test": "jest",
    "test:update": "jest -u",
    "test:coverage": "jest --coverage",
    "lint": "eslint --quiet",
    "lint:css": "stylelint --fix",
    "format": "prettier \"./src/**/*.{js,jsx,css,html}\" --write --only-changed"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run type:check && npm t"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ink8bit/ts-react-app.git"
  },
  "author": "ink8bit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ink8bit/ts-react-app/issues"
  },
  "homepage": "https://github.com/ink8bit/ts-react-app#readme",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@types/jest": "^24.0.21",
    "@types/reach__router": "^1.2.6",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-test-renderer": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.11.0",
    "style-loader": "^1.0.0",
    "stylelint": "^11.1.1",
    "stylelint-config-recommended": "^3.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "typescript": "^3.6.4",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  }
}
