{
  "name": "ts-react-app",
  "version": "0.21.0",
  "description": "TypeScript React app template",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "clean": "rm -rf build",
    "prebuild": "npm run clean",
    "prestart": "npm run build",
    "start": "NODE_ENV=production cd ./build && npx serve",
    "dev": "rollup -c -w",
    "build": "NODE_ENV=production rollup -c",
    "types:check": "tsc --noEmit",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx .",
    "lint:css": "stylelint \"src/**/*.css\"",
    "prettier": "prettier \"./src/**/*.{js,jsx,ts,tsx,css,html}\"",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --list-different",
    "test": "jest --forceExit",
    "test:update": "jest -u",
    "test:watch": "npm t -- --watch",
    "test:ci": "npm t -- --config jest.config.ci.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ink8bit/ts-react-app.git"
  },
  "author": "ink8bit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ink8bit/ts-react-app/issues"
  },
  "homepage": "https://github.com/ink8bit/ts-react-app#readme",
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-html": "^0.2.3",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-strip": "^2.0.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^26.0.23",
    "@types/reach__router": "^1.3.8",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "cssnano": "^5.0.6",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "postcss": "^8.3.5",
    "prettier": "^2.3.2",
    "react-test-renderer": "^16.14.0",
    "rollup": "^2.52.5",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-sizes": "^1.0.4",
    "rollup-plugin-terser": "^7.0.2",
    "serve": "^12.0.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "@reach/router": "^1.3.4",
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
