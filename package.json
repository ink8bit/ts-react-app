{
  "name": "ts-react-app",
  "version": "0.22.0",
  "description": "TypeScript React app template",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "clean": "rm -rf build",
    "prebuild": "npm run clean",
    "prestart": "npm run build",
    "start": "NODE_ENV=production cd ./build && npx serve",
    "dev": "rollup -c -w",
    "build": "NODE_ENV=production rollup -c",
    "type:check": "tsc --noEmit",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx .",
    "lint:css": "stylelint \"src/**/*.css\"",
    "prettier": "prettier \"./src/**/*.{js,jsx,ts,tsx,css,html}\"",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --list-different",
    "test": "jest --forceExit",
    "test:update": "jest -u",
    "test:watch": "npm t -- --watch",
    "test:ci": "npm t -- --config jest.config.ci.js",
    "prepare": "node ./prepare-hooks.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ink8bit/ts-react-app.git"
  },
  "author": "ink8bit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ink8bit/ts-react-app/issues"
  },
  "homepage": "https://github.com/ink8bit/ts-react-app#readme",
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-html": "^0.2.4",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.1.2",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-strip": "^2.1.0",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^18.0.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.19.0",
    "cssnano": "^5.1.7",
    "eslint": "^8.14.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.4.1",
    "node-notifier": "^10.0.1",
    "postcss": "^8.4.8",
    "prettier": "^2.6.2",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-sizes": "^1.0.4",
    "rollup-plugin-terser": "^7.0.2",
    "serve": "^13.0.2",
    "stylelint": "^14.6.1",
    "stylelint-config-recommended": "^7.0.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0"
  }
}
