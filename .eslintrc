{
  "extends": [
    "plugin:flowtype/recommended",
    "prettier",
    "airbnb-base"
  ],
  "plugins": [
    "flowtype",
    "prettier"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "jest": true,
    "node": true
  }
}
